#Code

fun main() {
    val any:Any = "Shubham"
    when(any) {
        "Shubham" -> { println("name") }
        "Rattra" -> { println("Sir name") }
        else -> { print("idk") }
    }
}


#Output

name



#Code

fun main() {
    val any:Any = "Shubham"
    val x = when(any) {
        "Shubham" -> "$any Rattra" 
        "Rattra" -> "Shubham $any"
        else -> "enter a valid name"
    }
    println(x)
}

#Output

Shubham Rattra
